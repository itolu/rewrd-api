{
    "name": "rewrd-api",
    "version": "1.0.0",
    "description": "Rewrd API v1",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon src/server.ts",
        "build": "tsc",
        "start": "npm run build && node dist/server.js",
        "lint": "eslint src/**/*.ts",
        "format": "prettier --write src/**/*.ts",
        "migrate:make": "knex migrate:make -x ts",
        "migrate:latest": "knex migrate:latest",
        "migrate:rollback": "knex migrate:rollback",
        "seed:make": "knex seed:make -x ts",
        "seed:run": "knex seed:run",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^30.0.0",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.12.7",
        "@types/supertest": "^6.0.3",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "axios": "^1.13.4",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-rate-limit": "^8.2.1",
        "helmet": "^7.1.0",
        "ioredis": "^5.9.3",
        "knex": "^3.1.0",
        "morgan": "^1.10.0",
        "nanoid": "^5.1.6",
        "pg": "^8.11.5",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "winston": "^3.19.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "eslint": "^9.0.0",
        "jest": "^30.2.0",
        "nodemon": "^3.1.0",
        "pg-mem": "^3.0.8",
        "prettier": "^3.2.5",
        "supertest": "^7.2.2",
        "ts-jest": "^29.4.6"
    }
}
