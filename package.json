{
    "name": "rewrd-api",
    "version": "1.0.0",
    "description": "Rewrd API v1",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "lint": "eslint src/**/*.ts",
        "format": "prettier --write src/**/*.ts",
        "migrate:make": "knex migrate:make -x ts",
        "migrate:latest": "knex migrate:latest",
        "migrate:rollback": "knex migrate:rollback",
        "seed:make": "knex seed:make -x ts",
        "seed:run": "knex seed:run"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "helmet": "^7.1.0",
        "knex": "^3.1.0",
        "morgan": "^1.10.0",
        "pg": "^8.11.5",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.12.7",
        "eslint": "^9.0.0",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    }
}